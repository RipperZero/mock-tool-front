{
  "name": "mock-tool-front",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --fix src",
    "test": "vitest --ui",
    "coverage": "vitest run --coverage",
    "preview": "vite preview",
    "build": "tsc && vite build",
    "check-circular": "npx madge --circular src",
    "output-dependencies": "npx madge --image dependencies.svg src",
    "remove-node_modules": "rimraf ./node_modules",
    "delete built": "rimraf ./dist"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix src"
    ],
    "**/*.{js,jsx,tsx,ts,css,less,scss,sass,md,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
