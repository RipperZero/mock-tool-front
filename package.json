    "@total-typescript/ts-reset": "0.6.1",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/node": "24.0.3",
    "@types/qs": "6.14.0",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react-swc": "3.10.2",
