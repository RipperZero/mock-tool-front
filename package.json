    "antd": "5.26.1",
    "axios": "1.10.0",
    "clipboard": "2.0.11",
    "clsx": "2.1.1",
    "dayjs": "1.11.13",
    "immer": "10.1.1",
    "qs": "6.14.0",
    "radash": "12.1.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "6.0.0",
    "react-hook-form": "7.58.1",
