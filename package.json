    "madge": "8.0.0",
    "picocolors": "1.1.1",
    "prettier": "3.5.3",
    "prettier-plugin-tailwindcss": "0.6.13",
    "rimraf": "6.0.1",
    "rollup-plugin-visualizer": "6.0.3",
    "tailwindcss": "4.1.10",
    "typescript": "5.8.3",
    "typescript-eslint": "8.34.1",
    "typescript-plugin-css-modules": "5.1.0",
